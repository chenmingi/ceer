{
  "manifest_version": 2,

  "name": "Cambio USD to BRL",
  "description": "This extension shows exchange rate USD/BRL in dynamic icon.",
  "version": "1.0",

  "browser_action": {
    "default_icon": "dolar.png",
	"default_popup": "popup.html"
  },
  "permissions": [
    "tabs"
  ],
  "background": { "page":"background.html"}
}